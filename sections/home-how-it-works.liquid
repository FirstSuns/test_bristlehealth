          },
          {
            "type": "url",
            "id": "btn_link_02",
            "label": "Button Link 02"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "Home How It Works",
        "category": "Text"
      }
    ]
  }
{% endschema %}

